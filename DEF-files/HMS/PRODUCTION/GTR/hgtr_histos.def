####################
# TARGET VARIABLES
####################

TH1F hgtr_mom  'HMS GTR Track Momentum; P (GeV); Number of Entires / 50 MeV' H.gtr.p 240 0 12 hcut_CleanTrack
TH1F hgtr_beta 'HMS GTR Track Beta; Beta (GeV); Number of Entires' H.gtr.beta 40 0.8 1.2 hcut_CleanTrack

TH1F hgtr_xtar 'HMS Golden Track X_{tar}; X_{tar} (cm); Number of Entries / 0.05 cm' H.gtr.x 80 -2 2 hcut_CleanTrack
TH1F hgtr_ytar 'HMS Golden Track Y_{tar}; Y_{tar} (cm); Number of Entries / 0.25 cm' H.gtr.y 96 -12.0 12.0 hcut_CleanTrack
TH1F hgtr_ztar 'HMS Golden Track Z_{tar}; Z_{tar} (cm); Number of Entries / 0.5 cm' H.react.z 100 -25.0 25.0 hcut_CleanTrack

TH2F hgtr_xtar_vs_reactz 'HMS Golden Track X_{tar}; Z_{react} / 0.5 cm; X_{tar} / 0.05 cm' H.react.z H.gtr.x 100 -25.0 25.0 80 -2.0 2.0 hcut_CleanTrack
TH2F hgtr_xtar_vs_ytar   'HMS Golden Track X_{tar} vs Y_{tar}; Y_{tar} (cm) / 0.25 cm; X_{tar} (cm) / 0.25 cm' H.gtr.y H.gtr.x 60 -7.5 7.5 96 -12.0 12.0 hcut_CleanTrack
TH2F hgtr_xptar_vs_yptar 'HMS Golden Track X'_{tar} vs Y'_{tar}; Y'_{tar} / 0.002 rad; X'_{tar} / 0.002 rad' H.gtr.ph H.gtr.th 100 -.1 .1 100 -.1 .1 hcut_CleanTrack
TH2F hgtr_xtar_vs_yptar  'HMS Golden Track X_{tar} vs Y'_{tar}; Y'_{tar} / 0.002 rad; X_{tar} / 0.25 cm'  H.gtr.ph H.gtr.x 100 -.1 .1 60 -7.5 7.5 hcut_CleanTrack
TH2F hgtr_xtar_vs_xptar  'HMS Golden Track X_{tar} vs X'_{tar}; X'_{tar} / 0.002 rad; X_{tar} / 0.25 cm' H.gtr.th H.gtr.x 100 -.1 .1 60 -7.5 7.5 hcut_CleanTrack
TH2F hgtr_ytar_vs_yptar  'HMS Golden Track Y_{tar} vs Y'_{tar}; Y'_{tar} / 0.002 rad; Y_{tar} / 0.25 cm'  H.gtr.ph H.gtr.y 100 -.1 .1 96 -12.0 12.0 hcut_CleanTrack
TH2F hgtr_ytar_vs_xptar  'HMS Golden Track Y_{tar} vs X'_{tar}; X'_{tar} / 0.002 rad; Y_{tar} / 0.25 cm' H.gtr.th H.gtr.y 100 -.1 .1 96 -12.0 12.0 hcut_CleanTrack